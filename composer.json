{
    "name": "micc83/rooles",
    "version": "3.0.0",
    "description": "A very simple roles and permissions manager for Laravel 5",
    "keywords": [
        "laravel",
        "RBAC",
        "ACL",
        "Roles",
        "Permissions"
    ],
    "license": "MIT",
    "homepage": "https://github.com/micc83/rooles",
    "support": {
        "issues": "https://github.com/micc83/rooles/issues",
        "source": "https://github.com/micc83/rooles"
    },
    "authors": [
        {
            "name": "Alessandro Benoit",
            "email": "micc83@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rooles\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "laravel/laravel": "^7.0|^8.0|^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
